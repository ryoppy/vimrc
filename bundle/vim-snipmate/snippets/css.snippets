################# スニペット #################
snippet sn_bg
	background: transparent url(${1}.png) repeat scroll 0 0;
snippet sn_shadow
	-webkit-box-shadow: ${1:x=0}px ${2:y=0}px ${3:size=8}px ${4:offset=0}px #${5:777};
	-moz-box-shadow   : $1px $2px $3px $4px #$5;
	box-shadow        : $1px $2px $3px $4px #$5;
################# 入力補助スニペット #################
snippet comm
	/* ----- ${1} ----- */
